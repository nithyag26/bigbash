

<body class =log>


<h2>Employee Login</h2> </br>


	<div class="form">
<%= form_tag emp_sessions_path do %>
  <p>
    
    <%= text_field_tag :name, nil , :placeholder => "Employee Name" %>
  </p>
  <p>
  
    <%= password_field_tag :password , nil , :placeholder => "Employee Password" %>
  </p>
  <p><%= submit_tag "Log in" , class: "btn-user" %></p>

  </div>
<% end %>

</body>