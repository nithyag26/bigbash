
<body class =log>

<h2>Admin Login</h2> </br>

<div class= "form">

<%= form_tag admin_sessions_path do %>
  <p>
    
    <%= text_field_tag :email, nil , :placeholder => "Email" %>
  </p>
  <p>
  
    <%= password_field_tag :password , nil , :placeholder => "Password" %>
  </p>
  <p><%= submit_tag "Log in" , class: "btn-user" %></p>
<% end %>


</div>
</body>

